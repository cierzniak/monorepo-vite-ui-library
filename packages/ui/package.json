{
  "name": "ui",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"tsc --watch\" \"vite build --watch\" \"start-storybook --port 6006 --no-open\"",
    "build": "concurrently \"tsc && vite build\" \"build-storybook --output-dir dist-docs\""
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-interactions": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/react": "^6.5.8",
    "@storybook/testing-library": "^0.0.12",
    "@types/node": "^16.11.39",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-loader": "^8.2.5",
    "sass": "^1.52.3",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vite-plugin-dts": "^1.2.0",
    "vite-plugin-libcss": "^1.0.5"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.js",
